{
  "name": "laravel-vue-easyforms",
  "version": "0.3.9-beta",
  "description": "Front end npm package for the Laravel EasyForms package",
  "author": {
    "name": "PlusTimeIT",
    "email": "kane@plustime.com.au",
    "url": "https://plustime.com.au/"
  },
  "contributors": [
    {
      "name": "LionEagleSolutions",
      "email": "james@lioneagle.solutions",
      "url": "https://lioneagle.solutions/"
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "main": "dist/laravel-vue-easyforms.common.js",
  "module": "dist/laravel-vue-easyforms.es.js",
  "unpkg": "dist/laravel-vue-easyforms.min.js",
  "exports": {
    ".": {
      "import": "./dist/laravel-vue-easyforms.es.js",
      "require": "./dist/laravel-vue-easyforms.common.js"
    }
  },
  "browser": {
    "./sfc": "src/components/forms/FormLoader.vue"
  },
  "keywords": [
    "PlusTime IT",
    "LionEagleSolutions",
    "laravel forms",
    "laravel",
    "vue"
  ],
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.md"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0",
    "moment-timezone": "^0.5.33",
    "sass": "^1.49.8",
    "unplugin-vue-components": "^0.24.1",
    "vee-validate": "^3.4.5",
    "vue-async-computed": "^3.9.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.2.0",
    "eslint": "^8.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^1.19.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^12.6.0",
    "vite": "^4.3.8",
    "vue": "^2.6.12",
    "vuetify-loader": "^1.7.3"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "bugs": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms/issues",
  "license": "MIT",
  "repository": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms.git"
}
