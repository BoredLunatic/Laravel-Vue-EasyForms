{
  "name": "laravel-vue-easyforms",
  "version": "0.4.2-beta",
  "description": "Front end npm package for the Laravel EasyForms package",
  "author": "PlusTimeIT <kane@plustime.com.au> (https://plustime.com.au/)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run type:build && npm run story:build",
    "serve": "vite preview",
    "files": "vue-tsc --listFiles",
    "type:build": "vue-tsc --project tsconfig.build-types.json --declaration",
    "type:check": "vue-tsc --project tsconfig.build-types.json",
    "test": "vitest --ui",
    "lint:report": "eslint . --ext .ts,.tsx,.vue",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.vue",
    "prettier": "prettier .",
    "bench": "vitest bench",
    "coverage": "vitest run --coverage",
    "preview": "vite preview --outDir ./coverage",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "exports": {
    ".": "./dist"
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "laravel easy forms",
    "laravel forms",
    "laravel",
    "laravel 10",
    "vue",
    "vue3",
    "vuetify",
    "PlusTime IT"
  ],
  "files": [
    "dist/*",
    "*.md"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "cross-env": "^7.0",
    "maska": "^2.1.10",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "@babel/types": "^7.22.11",
    "@histoire/plugin-vue": "^0.17.1",
    "@mdi/font": "^7.2.96",
    "@mdit/plugin-container": "^0.4.8",
    "@rollup/plugin-json": "^6.0.0",
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/browser": "^0.34.3",
    "@vitest/coverage-istanbul": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "histoire": "^0.17.2",
    "histoire-vuetify-design-system": "^0.1.1",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.65.0",
    "sass-loader": "^13.2.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-vue": "^0.0.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.3",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.0.0",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8",
    "vue-types": "^5.1.1",
    "vuetify": "^3.3.17",
    "webdriverio": "^8.15.9"
  },
  "peerDependencies": {
    "vue": "^3.3.4",
    "vuetify": "^3.3.7"
  },
  "bugs": {
    "url": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PlusTimeIT/Laravel-Vue-EasyForms.git"
  },
  "homepage": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms#readme"
}
