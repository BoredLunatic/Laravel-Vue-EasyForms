{
  "name": "laravel-vue-easyforms",
  "version": "0.4.31",
  "description": "Front end npm package for the Laravel EasyForms package",
  "author": "PlusTimeIT <kane@plustime.com.au> (https://plustime.com.au/)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "setup": "./.scripts/install.sh",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build": "./.scripts/build.sh --non-interactive",
    "build:vite": "rimraf dist && vite build",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration",
    "build:story": "histoire build",
    "type:check": "vue-tsc --project tsconfig.build-types.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:bench": "vitest bench",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview --outDir ./coverage",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "maska": "^2.1.10",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.1",
    "@mdi/font": "^7.2.96",
    "@mdit/plugin-container": "^0.4.8",
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/browser": "^0.34.3",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.3",
    "@vitest/utils": "^0.34.6",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-vuetify": "^2.0.5",
    "happy-dom": "^12.10.3",
    "histoire": "^0.17.5",
    "histoire-vuetify-design-system": "^0.1.3",
    "jsdom": "^23.0.0",
    "prettier": "^2.8.8",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.65.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.8",
    "vuetify": "^3.3.17",
    "webdriverio": "^8.15.9"
  },
  "peerDependencies": {
    "vue": "^3.3.4",
    "vuetify": "^3.3.7"
  },
  "bugs": {
    "url": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PlusTimeIT/Laravel-Vue-EasyForms.git"
  },
  "homepage": "https://github.com/PlusTimeIT/Laravel-Vue-EasyForms#readme",
  "keywords": [
    "laravel easy forms",
    "laravel forms",
    "laravel",
    "laravel 10",
    "vue",
    "vue3",
    "vuetify",
    "PlusTime IT"
  ],
  "files": [
    "dist/*",
    "*.md"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "actions": [
        "dist/actions.d.ts"
      ],
      "elements": [
        "dist/elements.d.ts"
      ],
      "fields": [
        "dist/fields.d.ts"
      ],
      "forms": [
        "dist/forms.d.ts"
      ],
      "properties": [
        "dist/properties.d.ts"
      ],
      "server": [
        "dist/server.d.ts"
      ],
      "enums": [
        "dist/enums.d.ts"
      ],
      "types": [
        "dist/types.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./actions": {
      "import": "./dist/actions.js"
    },
    "./elements": {
      "import": "./dist/elements.js"
    },
    "./fields": {
      "import": "./dist/fields.js"
    },
    "./forms": {
      "import": "./dist/forms.js"
    },
    "./properties": {
      "import": "./dist/properties.js"
    },
    "./server": {
      "import": "./dist/server.js"
    },
    "./enums": {
      "import": "./dist/enums.js"
    },
    "./types": {
      "import": "./dist/types.js"
    }
  },
  "stackblitz": {
    "startCommand": "npm run story:dev"
  }
}
